                <EditText
                    android:id="@+id/b4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Alexander Seeliger, Msc" />

                <EditText
                    android:id="@+id/b5"
                    android:layout_width="wrap_content"
